const arr = [1,2,-5,3,4,5,6,-5,-7,9,-6,2,26,33,69,75]
    let num3 = 0
    let num2 = 0
    
    for (let i = 0; i < arr.length; i++ ){
       if (arr[i] %2 ==0) {
        num3 = num3+arr[i]
        console.log(num3);
       } else {
         num2=num2+arr[i]
         console.log(num2);
       }
        } 
    console.log(num2,num3);
    
    let result = 0;
     if (num2>num3) {
        result=num2-num3
     } else {
        result=num3-num2
        
     }
     console.log(result);
